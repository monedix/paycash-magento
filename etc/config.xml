<?xml version="1.0"?>
<!--
 
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">    
    <default>
        <csp>
            <mode>
                <storefront>
                    <report_only>0</report_only>
                </storefront>
                <admin>
                    <report_only>0</report_only>
                </admin>
            </mode>
        </csp>        
        <payment>
            <paycash_pay>
                <model>Paycash\Pay\Model\Payment</model>
                <sandbox_sk backend_model="Magento\Config\Model\Config\Backend\Encrypted" />                
                <live_sk backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <order_status>pending</order_status>
                <pc_activo>0</pc_activo>
                <pc_pruebas>1</pc_pruebas>
                <pc_mensaje>Paga en efectivo con PayCash</pc_mensaje> 
                <pc_apitestkey></pc_apitestkey>
                <pc_apiproductionkey></pc_apiproductionkey>
                <pc_pais>MX</pc_pais>                
                <pc_diasvigencia>3</pc_diasvigencia>
                <pc_descripcion>Realiza tu pago en efectivo en cualquiera de los establecimientos afiliados a PayCash con la referencia mostrada a continuación</pc_descripcion>
                <pc_instrucciones>Realiza tu pago en efectivo en cualquiera de los establecimientos afiliados a PayCash con la referencia mostrada a continuación</pc_instrucciones>                                             
            </paycash_pay>
        </payment>
    </default>
</config>
